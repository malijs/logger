{
  "name": "@malijs/logger",
  "version": "0.5.0",
  "description": "Development logging middleware for Mali",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/malijs/logger.git"
  },
  "author": {
    "name": "Bojan D.",
    "email": "dbojan@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/malijs/logger/issues"
  },
  "homepage": "https://github.com/malijs/logger",
  "keywords": [
    "mali",
    "grpc",
    "service",
    "server",
    "microservice"
  ],
  "scripts": {
    "test": "jest --detectOpenHandles",
    "release": "semantic-release"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "is-stream": "^2.0.0"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.2.8",
    "@grpc/proto-loader": "^0.5.6",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "highland": "^3.0.0-beta.9",
    "jest": "^26.6.3",
    "JSONStream": "^1.3.5",
    "lodash": "^4.17.20",
    "mali": "^0.40.0",
    "pify": "^5.0.0",
    "semantic-release": "^17.3.9",
    "sinon": "^9.2.4",
    "sprom": "^3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/npm",
    [
      "@semantic-release/git",
      {
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ],
  "release": {
    "branches": ["master"]
  }
}
